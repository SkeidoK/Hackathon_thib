{% block map %}
<div>
<form action="http://localhost:8000/search" method=GET>
    <label for="ville"></label>
    <input type="text" id="ville" name="ville">
</form>
</div>
<div class ="d-flex justify-content-end me-5">
<iframe src="https://map.openchargemap.io/?mode=embedded&latitude={{ y|default("47") }}&longitude={{ x|default("1.9") }}" allow="geolocation" frameborder="0" width="45%" height="500px"></iframe>
</div>
{% endblock %}
